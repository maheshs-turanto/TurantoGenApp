@model PagedList.IPagedList<GeneratorBase.MVC.Models.T_ExportDataLog>
@using PagedList.Mvc;
@{
    ViewBag.Title = "Index";
    Layout = null;
    Response.ContentType = "application/vnd.ms-excel";
    Response.AddHeader("Content-Disposition", "attachment; filename=\"Export Data Log.xls\"");
}
                    <table>
                            <tr>
@if(User.CanView("T_ExportDataLog","T_AutoNo"))
{
		<th>Auto No.</th>
}
@if(User.CanView("T_ExportDataLog","T_ExportDataConfigurationExportDataLogAssociationID"))
{
		<th  @(Convert.ToString(ViewData["HostingEntity"]) == "T_ExportDataConfiguration"?"hidden":"")>Export Data Configuration</th>
}
@if(User.CanView("T_ExportDataLog","T_Tag"))
{
		<th>Tag</th>
}
@if(User.CanView("T_ExportDataLog","T_Notes"))
{
		<th>Notes</th>
}
@if(User.CanView("T_ExportDataLog","T_AssociatedExportDataLogStatusID"))
{
		<th  @(Convert.ToString(ViewData["HostingEntity"]) == "T_ExportDataLogstatus"?"hidden":"")>Status</th>
}
    </tr>
@foreach (var item in Model) {
List<string> hiddenProperties = new List<string>();
if (User.businessrules.Any(p => p.EntityName == "T_ExportDataLog" && p.ActionTypeID.Contains(6)))
{
	hiddenProperties = item.ApplyHiddenRule(User.businessrules, "T_ExportDataLog");
}
    <tr>
	@if(User.CanView("T_ExportDataLog","T_AutoNo"))
{
		<td>
            @Html.DisplayFor(modelItem => item.T_AutoNo)
        </td>
}
@if(User.CanView("T_ExportDataLog","T_ExportDataConfigurationExportDataLogAssociationID"))
{
		<td @(Convert.ToString(ViewData["HostingEntity"]) == "T_ExportDataConfiguration"?"hidden":"")>
            @Html.DisplayFor(modelItem => item.t_exportdataconfigurationexportdatalogassociation.DisplayValue)
        </td>
}
@if(User.CanView("T_ExportDataLog","T_Tag"))
{
		<td>
            @Html.DisplayFor(modelItem => item.T_Tag)
        </td>
}
@if(User.CanView("T_ExportDataLog","T_Notes"))
{
		<td>
            @Html.DisplayFor(modelItem => item.T_Notes)
        </td>
}
@if(User.CanView("T_ExportDataLog","T_AssociatedExportDataLogStatusID"))
{
		<td @(Convert.ToString(ViewData["HostingEntity"]) == "T_ExportDataLogstatus"?"hidden":"")>
            @Html.DisplayFor(modelItem => item.t_associatedexportdatalogstatus.DisplayValue)
        </td>
}
                                </tr>
                            }
</table>
               
			   
