                                                @{
                                                    var WFHistoryT_AssociatedExportDataLogStatus = ((List<GeneratorBase.MVC.Models.JournalEntry>)ViewBag.JournalEntry).Where(p => p.Type == "Added" || p.PropertyName == "Status");
