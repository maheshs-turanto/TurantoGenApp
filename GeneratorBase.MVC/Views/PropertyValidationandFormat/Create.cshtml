@model GeneratorBase.MVC.Models.PropertyValidationandFormat
@{
    ViewBag.Title = "Create Property Validation and Format";
    var EntityDisplayNameReflector = ModelReflector.Entities.FirstOrDefault(p => p.Name == "PropertyValidationandFormat");
    var EntityDisplayName = EntityDisplayNameReflector != null ? EntityDisplayNameReflector.DisplayName : "Property Validation and Format";
    if (ViewBag.IsPartial != null && ViewBag.IsPartial == true)
    {
        Layout = null;
    }
}
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-8">
        @{
            var hostingValue = "";
            if (ViewBag.DisplayVal != null)
            {
                hostingValue = " - " + ViewBag.DisplayVal;
            }
        }
        <ol class="breadcrumb">
            <li class="active">
                <strong><a href="@Url.Action("Index", "Admin")"> Admin</a> <a href="@Url.Action("Index")"> @EntityDisplayName</a></strong>
            </li>
            <li class="active">
                Add Property Validation and Format @hostingValue
            </li>
            <li class="active">
                <span>@Html.DisplayFor(model => model.DisplayValue) </span>
            </li>
        </ol>
    </div>
    <div class="col-lg-4">
        <div class="pull-right">
            @if (ViewBag.IsPartial == null)
            {
                if (ViewData["HostingEntity"] == null)
                {
                    <a href="@(Url.Action("SetFSearch", "PropertyValidationandFormat")+Request.Url.Query)" data-original-title="Query" class="btn btn-primary btn-xs" alt="Query" title="Query"><i class="glyphicon glyphicon-search"></i> Query</a>
                }
            }
            @if (ViewBag.IsPartial == null)
            {
                <a class="btn btn-primary btn-xs" type="button" data-original-title="List View" data-placement="top" onclick=@Html.Raw("NavigateToUrl('" + Url.Action("Index", "PropertyValidationandFormat") + "');") alt="Search" title="List">
                    <i class="glyphicon glyphicon-list"></i> List
                </a>
            }

        </div>
    </div>
    <!-- /.col-lg-12 No Email-->
</div>
<div class="wrapper wrapper-content animated fadeInRight ecommerce">
    <div class="ibox">
        <div class="ibox-content">
            @Html.Partial("~/Views/PropertyValidationandFormat/CreatePartial.cshtml", Model)
        </div>
    </div>
</div>
