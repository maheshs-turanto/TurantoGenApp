@model IEnumerable<GeneratorBase.MVC.Models.EntityHelpPage>
@{
    ViewBag.Title = "Entity Help Page List";
	string templatename = ("~/Views/EntityHelpPage/" + ViewBag.TemplatesName + ".cshtml").Replace("?IsAddPop=true", "");
	 var EntityDisplayNameReflector = ModelReflector.Entities.FirstOrDefault(p => p.Name == "EntityHelpPage");
    var EntityDisplayName = EntityDisplayNameReflector != null ? EntityDisplayNameReflector.DisplayName : "Entity Help Page";
}
<div class="content-heading">
    <div class="col-lg-10">

        <strong>
            <a href="@Url.Action("Index")"> @EntityDisplayName</a>

            List
        </strong>
    </div>
    <div class="col-lg-2">
    </div>
</div>
<script>
    $(document).ready(function () {
        ClickFilterBtn();
    });
</script>
 <div class="wrapper wrapper-content animated fadeInRight ecommerce">
         <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
<div id="dvEntityHelpPageFilter"></div>
@Html.Partial(templatename, Model)
</div>
</div>
</div>
</div>
</div>
