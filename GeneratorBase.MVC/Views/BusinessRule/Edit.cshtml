@model GeneratorBase.MVC.Models.BusinessRule
@using PagedList;
@{
    ViewBag.Title = "Edit Business Rule";
    string displaychk = "display:none;";
    string layoutcol = "6";
    Type controller = Type.GetType("GeneratorBase.MVC.Controllers.NotificationController, GeneratorBase.MVC.Controllers");
    if (controller != null)
    {
        displaychk = "";
        layoutcol = "4";
    }
}
@{
    var actionListItems = new List<SelectListItem>()
{
        new SelectListItem() {Text="Make Record Readonly", Value="1"},
        new SelectListItem() {Text="Make Record & Associations Readonly", Value="11"},
        new SelectListItem() {Text="Make Fields Readonly", Value="2"},
        new SelectListItem() {Text="Make Fields Mandatory", Value="3"},
        new SelectListItem() {Text="Filter Dropdown", Value="5"},
        new SelectListItem() {Text="Restrict Dropdown", Value="17"},
        new SelectListItem() {Text="Email Notification", Value="4"},
        new SelectListItem() {Text="Make Fields Hidden", Value="6"},
        new SelectListItem() {Text="Make Groups/Tabs Hidden", Value="12"},
        new SelectListItem() {Text="Set Value", Value="7"},
        new SelectListItem() {Text="Invoke Action", Value="8"},
        new SelectListItem() {Text="Validate Before Save", Value="10"},
        new SelectListItem() {Text="UI Alert", Value="13"},
        new SelectListItem() {Text="Confirmation Before Save", Value="15"},
        new SelectListItem() {Text="Make Verb(s) Hidden", Value="16"},

    };
}

<script>

    //ulActionList
    $(document).ready(function () {
        $("#ulActionList li").each(function (index, value) {
            var input = $(this).find('input');
            var span = $(this).find('span');
            if (input.val() == "1") {
                span.after().append("<p>People won't be able to edit the record. This is like locking down the record to prevent any changes.</p>");
            }

            if (input.val() == "2") {
                span.after().append("<p>People won't be able to edit selected properties of the records.</p>");
            }

            if (input.val() == "3") {
                span.after().append("<p>Makes the fields mandatory when condition is true, e.g.'Tax' field is mandatory when 'Base price' is not empty.</p>");
            }

            if (input.val() == "4") {

                span.after().append("<p>When condition is matched, an email notification is sent to specified email addresses or roles.</p>");
            }

            if (input.val() == "5") {
                span.after().append("<p>When you select the drop-down in the list, the drop-down will show only those records for which the evaluation criteria match. </p>");
            }

            if (input.val() == "6") {
                span.after().append("<p>Hide the fields (properties) from the entity.</p>");
            }

            if (input.val() == "7") {
                span.after().append("<p>Puts some value in the desired field, e.g. set current date when record status is changed to closed.</p>");
            }

            if (input.val() == "8") {
                span.after().append("<p>Executes the code of a verb (other than the default verbs provided in Turanto application).</p>");
            }

            if (input.val() == "10") {
                span.after().append("<p>Prevents the user from saving the record. We recommend to use this action with a 'Custom message'.</p>");
            }

            if (input.val() == "11") {
                span.after().append("<p>People won't be able to edit the records and any associated records in their tabs.</p>");
            }
            if (input.val() == "12") {
                span.after().append("<p>You can hide the groups and all the fields in it.</p>");
            }

            if (input.val() == "13") {
                span.after().append("<p>Shows a warning message (fill out Custom Message field) in the header and footer of the record when the condition is met, e.g. ‘Status’ has a value equal to 'Inactive'.</p>");
            }

            if (input.val() == "14") {
                span.after().append("<p>Call external web-api when condition satisfies e.g. add an entry in some other application through web-api when patient registers.</p>");
            }

            if (input.val() == "15") {
                span.after().append("<p>Shows a confirmation message (fill out Custom Message field) before save. This reduces the chance of accidentally saving wrong information.</p>");
            }

            if (input.val() == "16") {
                span.after().append("<p>Hides the selected (single action) verbs.</p>");
            }

            if (input.val() == "17") {
                span.after().append("<p>Restrict Dropdown value on selected entity page</p>");
            }
        });
    });

</script>
@{
    if (controller != null)
    {
        <script>
            $(document).ready(function () {
                $("#ulActionList li").each(function (index, value) {
                    var input = $(this).find('input');
                    if (input.val() == "4") {
                        var span = $(this).find('span');
                        span.html("Notification<p>When condition is matched, notification system sends email or alert to specified role or email address.</p>");
                    }
                });
            });
        </script>
    }
}

