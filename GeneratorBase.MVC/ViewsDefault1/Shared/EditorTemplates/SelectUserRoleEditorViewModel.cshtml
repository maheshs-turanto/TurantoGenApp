@model GeneratorBase.MVC.Models.SelectUserRoleEditorViewModel
@{
    var checkboxHtmlAttributes = new Dictionary<string, object>
    {{"attibute1", "value1"},
    {"attribute2", "value2"}};
    if (Model.isdisabled)
    {
        checkboxHtmlAttributes.Add("disabled", "disabled");
        checkboxHtmlAttributes.Add("readonly", "readonly");
        checkboxHtmlAttributes.Add("onclick", "return false;");
    }
   
}
<tr>
    @Html.HiddenFor(model => model.Id)
    @Html.HiddenFor(model => model.RoleName)
    <td>@Html.CheckBoxFor(model => model.Selected, checkboxHtmlAttributes)</td>
    <td>@Html.DisplayFor(model => model.RoleName)</td>
</tr>
